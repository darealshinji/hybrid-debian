#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_build:
	dh_auto_build -- prefix=/usr

override_dh_auto_install:
	dh_auto_install -- prefix=/usr

override_dh_installdocs:
	dh_installdocs -phybrid-common -phybrid-tools
	dh_installdocs -phybrid -phybrid-qt46 -phybrid-qt5 --link-doc=hybrid-common

override_dh_builddeb:
	dh_builddeb -- -Zxz -z9

